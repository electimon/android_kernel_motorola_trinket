#include "trinket-moto-common-overlay.dtsi"
#include "rav-panel.dtsi"
#include "trinket-rav-touch-overlay.dtsi"
#include "trinket-rav-audio-overlay.dtsi"
#include "trinket-rav-sensor-overlay.dtsi"
#include "trinket-rav-fps-overlay.dtsi"
#include "trinket-rav-camera-overlay.dtsi"

#define RAV_RADIO_NA 2

&key_vol_up_default {
	pins = "gpio1";
	power-source = <0>;
};

&soc {
	gpio_keys {
		vol_up {
			gpios = <&pm6125_gpios 1 GPIO_ACTIVE_LOW>;
		};
	};
};

&qupv3_se1_i2c {
	nq@28 {
		status = "disabled";
	};
};

&thermal_zones {
	rf-pa0-therm-adc {
		status = "disabled";
	};

	quiet-therm-step {
		status = "disabled";
	};

	pa_therm0 {
		polling-delay-passive = <0>;
		polling-delay = <0>;
		thermal-has-hwmon;
		thermal-governor = "user_space";
		thermal-sensors = <&pm6125_adc_tm ADC_AMUX_THM1_PU2>;
		trips {
			active-config0 {
				temperature = <125000>;
				hysteresis = <1000>;
				type = "passive";
			};
		};
	};

	quiet-therm-adc {
		status = "disabled";
	};
	quiet_therm {
		polling-delay-passive = <0>;
		polling-delay = <0>;
		thermal-has-hwmon;
		thermal-governor = "user_space";
		thermal-sensors = <&pm6125_adc_tm ADC_AMUX_THM2_PU2>;
		trips {
			active-config0 {
				temperature = <125000>;
				hysteresis = <1000>;
				type = "passive";
			};
		};
	};

	xo-therm-adc {
		status = "disabled";
	};
	xo_therm {
		polling-delay-passive = <0>;
		polling-delay = <0>;
		thermal-has-hwmon;
		thermal-governor = "user_space";
		thermal-sensors = <&pm6125_adc_tm ADC_XO_THERM_PU2>;
		trips {
			active-config0 {
				temperature = <125000>;
				hysteresis = <1000>;
				type = "passive";
			};
		};
	};

	conn-therm-adc {
		status = "disabled";
	};

	skin-therm-adc {
		status = "disabled";
	};

	chg_therm {
		polling-delay-passive = <0>;
		polling-delay = <0>;
		thermal-has-hwmon;
		thermal-governor = "user_space";
		thermal-sensors = <&pmi632_adc_tm ADC_GPIO2_PU2>;
		trips {
			active-config0 {
				temperature = <125000>;
				hysteresis = <1000>;
				type = "passive";
			};
		};
	};

	camera-ftherm-adc {
		status = "disabled";
	};
	msm_therm {
		polling-delay-passive = <0>;
		polling-delay = <5000>;
		thermal-has-hwmon;
		thermal-governor = "user_space";
		thermal-sensors = <&pm6125_adc_tm_iio ADC_GPIO1_PU2>;
		trips {
			active-config0 {
				temperature = <125000>;
				hysteresis = <1000>;
				type = "passive";
			};
		};
	};

	emmc-ufs-therm-adc {
		status = "disabled";
	};
};
